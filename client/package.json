    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^17.0.2",
    "react-google-login": "^5.2.2",
    "react-player": "^2.9.0",
    "react-toastify": "^7.0.4",
    "styled-components": "^5.3.0",
