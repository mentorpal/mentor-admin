{
  "name": "mentor-admin",
  "description": "web ui for recording mentorpal mentors",
  "version": "1.0.0",
  "private": true,
  "author": "Kayla Carr",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "format": "prettier --write '**/*.{js,jsx,json,ts,tsx}'",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "npx gatsby build --prefix-paths",
    "serve": "gatsby serve --prefix-paths",
    "clean": "gatsby clean",
    "license:fix": "license-check-and-add remove -f license-config.json && license-check-and-add add -f license-config.json",
    "test:license": "license-check-and-add check -f license-config.json",
    "test:audit": "npm audit --level=moderate",
    "test:format": "prettier --check '**/*.{js,jsx,json,ts,tsx}'",
    "test:lint": "eslint src --ext ts --ext tsx --ext js --ext jsx",
    "test:types": "tsc --noEmit"
  },
  "license": "0BSD",
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@reach/router": "^1.3.4",
    "clsx": "^1.1.1",
    "gatsby": "^2.26.1",
    "mongoose": "^5.11.17",
    "react": "^16.13.1",
    "react-beautiful-dnd": "^13.0.0",
    "react-cookie": "^4.0.3",
    "react-dom": "^16.13.1",
    "react-google-login": "^5.2.2",
    "react-material-ui-carousel": "^2.1.1",
    "react-player": "^2.7.0",
    "react-toastify": "^6.2.0",
    "react-video-recorder": "^3.17.1",
    "react-video-trimmer": "^1.3.0-0",
    "styled-components": "^5.2.1",
    "url-join": "^4.0.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/node": "^14.14.13",
    "@types/react-beautiful-dnd": "^13.0.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gatsby-plugin-env-variables": "^2.0.0",
    "gatsby-plugin-eslint": "^2.0.8",
    "gatsby-plugin-manifest": "^2.11.0",
    "gatsby-plugin-material-ui": "^2.1.10",
    "gatsby-plugin-sitemap": "^3.1.0",
    "gatsby-plugin-typescript": "^2.8.0",
    "license-check-and-add": "^3.0.4",
    "prettier": "^2.2.1",
    "typescript": "^4.1.2"
  }
}
