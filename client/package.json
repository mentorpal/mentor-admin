{
  "name": "mentor-admin",
  "description": "web ui for recording mentorpal mentors",
  "version": "1.0.0",
  "private": true,
  "author": "Kayla Carr",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "format": "prettier --write '**/*.{js,jsx,json,ts,tsx}'",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "npx gatsby build --prefix-paths",
    "serve": "gatsby serve --prefix-paths",
    "clean": "gatsby clean",
    "license:fix": "license-check-and-add remove -f license-config.json && license-check-and-add add -f license-config.json",
    "test:license": "license-check-and-add check -f license-config.json",
    "test:audit": "npm audit --production --audit-level=moderate",
    "test:format": "prettier --check '**/*.{js,jsx,json,ts,tsx}'",
    "test:lint": "eslint src --ext ts --ext tsx --ext js --ext jsx",
    "test:types": "tsc --noEmit"
  },
  "license": "0BSD",
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@reach/router": "^1.3.4",
    "clsx": "^1.1.1",
    "gatsby": "^3.3.0",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-cookie": "^4.0.3",
    "react-dom": "^17.0.2",
    "react-google-login": "^5.2.2",
    "react-player": "^2.9.0",
    "react-toastify": "^7.0.3",
    "react-video-recorder": "^3.17.2",
    "styled-components": "^5.2.3",
    "url-join": "^4.0.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/node": "^14.14.41",
    "@types/react-beautiful-dnd": "^13.0.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-react": "^7.23.2",
    "gatsby-plugin-env-variables": "^2.1.0",
    "gatsby-plugin-eslint": "^3.0.0",
    "gatsby-plugin-manifest": "^3.3.0",
    "gatsby-plugin-material-ui": "^3.0.1",
    "gatsby-plugin-sitemap": "^3.3.0",
    "gatsby-plugin-typescript": "^3.3.0",
    "license-check-and-add": "^4.0.2",
    "prettier": "^2.2.1",
    "typescript": "^4.2.4"
  }
}
