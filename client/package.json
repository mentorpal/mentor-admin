    "gatsby-plugin-env-variables": "^2.1.0",
    "gatsby-plugin-eslint": "^3.0.0",
    "gatsby-plugin-manifest": "^3.8.0",
    "gatsby-plugin-material-ui": "^3.0.1",
    "gatsby-plugin-sitemap": "^4.4.0",
    "gatsby-plugin-typescript": "^3.8.0",
    "typescript": "^4.3.5"
